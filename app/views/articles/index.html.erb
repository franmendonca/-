<h1 class= "page_title">Stuff I've written/done</h1>
<div id= "posts_wrapper" class= "skinny_wrapper">
  <% @articles.each do |article| %>
    <div class="post">
      <h2> <%= link_to "#{article.title}", article_path(article)%> </h2>
      <p class= "date"> <%= article.date%> </p>
      <hr>
    </div>
    <% end %>
</div>
